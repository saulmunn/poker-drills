#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/sucrase@3.35.0/node_modules/sucrase/bin/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/sucrase@3.35.0/node_modules/sucrase/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/sucrase@3.35.0/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/sucrase@3.35.0/node_modules/sucrase/bin/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/sucrase@3.35.0/node_modules/sucrase/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/sucrase@3.35.0/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sucrase-node" "$@"
else
  exec node  "$basedir/../../bin/sucrase-node" "$@"
fi
