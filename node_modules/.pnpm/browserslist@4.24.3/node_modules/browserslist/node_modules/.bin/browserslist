#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/code/node_modules/.pnpm/browserslist@4.24.3/node_modules/browserslist/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/code/node_modules/.pnpm/browserslist@4.24.3/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/code/node_modules/.pnpm/browserslist@4.24.3/node_modules/browserslist/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/code/node_modules/.pnpm/browserslist@4.24.3/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
