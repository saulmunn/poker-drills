#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/vite@6.0.6_jiti@1.21.7_yaml@2.7.0/node_modules/vite/bin/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/vite@6.0.6_jiti@1.21.7_yaml@2.7.0/node_modules/vite/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/vite@6.0.6_jiti@1.21.7_yaml@2.7.0/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/vite@6.0.6_jiti@1.21.7_yaml@2.7.0/node_modules/vite/bin/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/vite@6.0.6_jiti@1.21.7_yaml@2.7.0/node_modules/vite/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/vite@6.0.6_jiti@1.21.7_yaml@2.7.0/node_modules:/Users/saulmunn/Dropbox/My Mac (Saul’s MacBook Pro)/Documents/1.5 - Informal Edification/Poker/Coding/poker-drills/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
